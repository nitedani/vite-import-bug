{
	"compilerOptions": {
		"baseUrl": ".",
		"noImplicitAny": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"strict": true,
		"module": "esnext",
		"moduleResolution": "Node",
		"target": "esnext",
		"outDir": "dist",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"jsx": "react-jsx",
		"skipLibCheck": true,
		"esModuleInterop": true
	}
}